{
  "name": "@maomaolabs/core",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maomaolabs/core.git"
  },
  "version": "0.0.0",
  "type": "module",
  "main": "dist/core.umd.js",
  "module": "dist/core.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/core.es.js",
      "require": "./dist/core.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
